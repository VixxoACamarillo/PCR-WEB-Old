    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.29.4",
