    "@types/jquery": "^3.5.19",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "svg-sprite": "^2.0.2",
    "typescript": "~4.6.2"
  },
  "resolutions": {
    "uglify-es": "npm:terser"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,html,sass,scss}": [
      "prettier --write --config .prettierrc",
      "git add"
    ]
  }
}
