    "rxjs": "~7.5.0",
    "sass-flex-mixin": "^1.0.3",
    "tslib": "^1.9.0",
    "vixxo-ui": "https://s3.amazonaws.com/vixxo-shared-packages/npm/vixxo-ui-2.0.0.tgz",
