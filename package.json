{
  "name": "pcr-web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prebuild": "npm run sprites && node --max_old_space_size=5120 prebuild.js",
    "sprites": "svg-sprite --config sprite-config.json src/assets/images/svgs/*.svg",
    "start:deploy": "npm run build-prod && npm run start",
    "start:dev": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng serve",
    "start": "node --max_old_space_size=5120 server.js",
    "node-dev": "nodemon server.js",
    "build-dev": "npm run prebuild && ng build --configuration=dev",
    "build-prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --configuration=$VIXXO_ENV",
    "lint": "ng lint",
    "test-pipeline": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --aot --build-optimizer=true",
    "regression-validation": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng e2e -c=validation",
    "regression-preview": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng e2e -c=preview",
    "regression-staging": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng e2e -c=staging",
    "regression-local": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/localize": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@elastic/ecs-winston-format": "^1.3.1",
    "@ngrx/router-store": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@progress/kendo-angular-buttons": "^8.0.0",
    "@progress/kendo-angular-common": "^3.0.0",
    "@progress/kendo-angular-dateinputs": "^7.1.6",
    "@progress/kendo-angular-dialog": "^7.1.5",
    "@progress/kendo-angular-dropdowns": "^7.0.0",
    "@progress/kendo-angular-excel-export": "^5.0.2",
    "@progress/kendo-angular-grid": "^7.4.2",
    "@progress/kendo-angular-inputs": "^9.0.0",
    "@progress/kendo-angular-intl": "^4.0.0",
    "@progress/kendo-angular-l10n": "^4.0.0",
    "@progress/kendo-angular-label": "^4.0.2",
    "@progress/kendo-angular-layout": "^7.2.0",
    "@progress/kendo-angular-listview": "^4.0.4",
    "@progress/kendo-angular-pager": "^4.0.0",
    "@progress/kendo-angular-pdf-export": "^4.0.1",
    "@progress/kendo-angular-popup": "^5.0.0",
    "@progress/kendo-angular-progressbar": "^3.0.0",
    "@progress/kendo-angular-sortable": "^3.0.6",
    "@progress/kendo-angular-tooltip": "^2.1.6",
    "@progress/kendo-angular-treeview": "^7.0.0",
    "@progress/kendo-angular-upload": "^9.0.4",
    "@progress/kendo-data-query": "^1.6.0",
    "@progress/kendo-drawing": "^1.5.12",
    "@progress/kendo-licensing": "^1.3.2",
    "@progress/kendo-theme-default": "^5.0.0",
    "@types/auth0-js": "^9.21.0",
    "@types/jwt-decode": "^2.2.1",
